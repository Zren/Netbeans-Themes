<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <!-- (JavaDoc) -->
    
    <!-- javadoc -->
    <fontcolor name="javadoc-identifier" default="comment" foreColor="ffB3B5AF" />
    <fontcolor name="javadoc-first-sentence" default="comment" >
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="html-tag" default="comment" foreColor="ff9999ff" >
        <font style="bold"/>
    </fontcolor>
    
    <!-- javadocTag -->
    <fontcolor name="javadoc-tag" default="comment" foreColor="ff9393CC">
        <font style="bold"/>
    </fontcolor>
    
    <!-- (Identifier) -->
    <fontcolor default="identifier" name="identifier"/>
    <!-- enum -->
    <fontcolor name="mod-enum" default="identifier">
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="mod-enum-declaration" default="identifier">
        <font style="bold"/>
    </fontcolor>
    
    <!-- interface -->
    <fontcolor name="mod-interface" default="identifier" foreColor="ffCAE682" />
    <fontcolor name="mod-interface-declaration" default="identifier" >
        <font style="bold"/>
    </fontcolor>
    <!-- annotation -->
    <fontcolor name="mod-annotation-type" default="identifier" foreColor="ff808080" />
    <fontcolor name="mod-annotation-type-declaration" default="identifier" >
        <font style="bold"/>
    </fontcolor>
    
    <!-- class -->
    <fontcolor name="mod-class" default="identifier" foreColor="ffCAE682"/>
    <fontcolor name="mod-class-declaration" default="identifier" >
        <font style="bold"/>
    </fontcolor>

    <!-- (Static) [Netbeans does not seperate "staticField, staticFinalField, staticMethod"] -->
    <fontcolor name="mod-static">
        <font style="italic"/>
    </fontcolor>
    
    
    <!-- (Character) -->
    <fontcolor name="character" default="char" />
    <!-- (String Escape) -->
    <fontcolor name="string-escape" default="string" >
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="string-escape-invalid" default="string" waveUnderlined="ffce7b00">
        <font style="bold"/>
    </fontcolor>
    <!-- (Keywords) -->
    <fontcolor name="keyword-directive" default="keyword" />
    <fontcolor name="literal" default="keyword" />
    <!-- parameterVariable -->
    <fontcolor name="mod-parameter" default="identifier" />
    <fontcolor name="mod-parameter-declaration" default="identifier" />
    <!-- (Constructor) -->
    <fontcolor name="mod-constructor" default="identifier" />
    <fontcolor name="mod-constructor-declaration" default="identifier" >
        <font style="bold"/>
    </fontcolor>
    <!-- localVariable / localVariableDeclaration -->
    <fontcolor name="mod-local-variable" default="identifier" foreColor="ffD4C4A9"/>
    <fontcolor name="mod-local-variable-declaration" default="identifier" foreColor="ffD4C4A9"/>
    
    <!-- (Abstract) [Eclipse keyword] -->
    <fontcolor name="mod-abstract"/>
    <!-- (Visibility) [Eclipse keyword] -->
    <fontcolor name="mod-private"/>
    <fontcolor name="mod-protected"/>
    <fontcolor name="mod-public"/>

    <!-- deprecatedMember [Eclipse overrides the foreColor] -->
    <fontcolor name="mod-deprecated" strikeThrough="ffFFFFFF"/>
    
    
    <!-- occurrenceIndication -->
    <fontcolor name="mark-occurrences" bgColor="ff616161" />
    
    <!-- (Misc) -->
    <fontcolor name="mod-unused" waveUnderlined="ff999999"/>
    <fontcolor name="mod-package-private"/>
    
    <!--  -->
    <fontcolor name="comment" default="comment" />
    <fontcolor name="number" default="number" />
    <fontcolor name="separator" default="separator" />
    <fontcolor name="string" default="string" />
    <fontcolor name="keyword" default="keyword" />
    <fontcolor name="whitespace" default="whitespace" />
    <fontcolor name="operator" default="operator" />
    <fontcolor name="mod-method" default="method" />
    <fontcolor name="mod-method-declaration" default="method" >
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="mod-field" default="field" />
    <fontcolor name="mod-field-declaration" default="field" />

</fontscolors>
