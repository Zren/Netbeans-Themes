<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <!-- selectionForeground / selectionBackground -->
    <fontcolor name="selection" bgColor="ff898941" foreColor="ff000000" />
    
    <!-- lineNumber -->
    <fontcolor name="line-number" foreColor="ffC0C0C0" />
    
    <!-- currentLine -->
    <fontcolor name="highlight-caret-row" bgColor="ff505050" />

    <!-- (Caret) -->
    <fontcolor name="caret-color-insert-mode" foreColor="white"/>
    <fontcolor name="caret-color-overwrite-mode" foreColor="white"/>
    
    <!-- (Misc) -->
    <fontcolor name="text-limit-line-color" foreColor="ff221111" />
    
    <!-- findScope (Bracket Matching) -->
    <fontcolor name="nbeditor-bracesMatching-match" bgColor="ffBCADAD" />
    <fontcolor name="nbeditor-bracesMatching-match-multichar" bgColor="ffBCADAD" />
    <fontcolor name="nbeditor-bracesMatching-mismatch" bgColor="ffd83636" />
    <fontcolor name="nbeditor-bracesMatching-mismatch-multichar" bgColor="ffd83636" />
    
    <!-- (Read Only) -->
    <fontcolor name="readonly-files" />
    
    <!-- -->
    <fontcolor foreColor="ff666666" name="code-folding"/>
    <fontcolor bgColor="ffe0e8f1" foreColor="black" name="block-search"/>
    
    <fontcolor foreColor="blue" name="hyperlinks" underline="blue"/>
    <fontcolor foreColor="red" name="status-bar-bold"/>
    <fontcolor name="status-bar"/>
    <fontcolor bgColor="ffffb442" foreColor="black" name="inc-search"/>
    <fontcolor foreColor="ffcc0099" name="synchronized-text-blocks-ext"/>
    <fontcolor foreColor="ff666666" name="code-folding-bar"/>
    <fontcolor bgColor="ffffb442" foreColor="black" name="highlight-search"/>
    <fontcolor foreColor="ff996600" name="synchronized-text-blocks-ext-slave"/>
    <fontcolor bgColor="ffdfdfdf" name="guarded"/>
</fontscolors>