<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <!-- selectionForeground / selectionBackground -->
    <fontcolor name="selection" bgColor="ff000000" foreColor="ffDDF0FF" />
    
    <!-- lineNumber -->
    <fontcolor name="line-number" foreColor="ffF9F9F9" />
    
    <!-- currentLine -->
    <fontcolor name="highlight-caret-row" bgColor="ff2F2F2F" />

    <!-- (Caret) -->
    <fontcolor name="caret-color-insert-mode" foreColor="ffFFFFFF"/>
    <fontcolor name="caret-color-overwrite-mode" foreColor="ffFFFFFF"/>
    
    <!-- findScope (Bracket Matching) -->
    <fontcolor name="nbeditor-bracesMatching-match" bgColor="ffDDF0FF" />
    <fontcolor name="nbeditor-bracesMatching-match-multichar" bgColor="ffDDF0FF" />
    <fontcolor name="nbeditor-bracesMatching-mismatch" bgColor="ffd83636" />
    <fontcolor name="nbeditor-bracesMatching-mismatch-multichar" bgColor="ffd83636" />
    <fontcolor name="nbeditor-bracesMatching-sidebar" foreColor="ff6b8eb2" />
    
    <!-- (Read Only) -->
    <fontcolor name="readonly-files" />
    
    <!-- (Misc) -->
    <fontcolor name="text-limit-line-color" foreColor="ff221111" />
    
    <!-- -->
    <fontcolor name="code-folding" foreColor="ff666666" />
    <fontcolor name="block-search" bgColor="ffe0e8f1" foreColor="ff000000" />
    
    <fontcolor name="hyperlinks" foreColor="blue" underline="blue" />
    <fontcolor name="status-bar"/>
    <fontcolor name="status-bar-bold" foreColor="red" />
    <fontcolor name="inc-search" bgColor="ffffb442" foreColor="ff000000" />
    <fontcolor name="synchronized-text-blocks-ext" foreColor="ffcc0099" />
    <fontcolor name="code-folding-bar" foreColor="ff666666" />
    <fontcolor name="highlight-search" bgColor="ffffb442" foreColor="ff000000" />
    <fontcolor name="synchronized-text-blocks-ext-slave" foreColor="ff996600" />
    <fontcolor name="guarded" bgColor="ffdfdfdf" />
</fontscolors>