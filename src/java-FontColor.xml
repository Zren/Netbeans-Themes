<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <!-- (JavaDoc) -->
    
    <!-- javadoc -->
    <fontcolor name="javadoc-identifier" default="comment" foreColor="ff000000" />
    <fontcolor name="javadoc-first-sentence" default="javadoc-identifier">
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="html-tag" default="markup-element">
        <font style="bold"/>
    </fontcolor>
    
    <!-- javadocTag [Eg: @param]  -->
    <fontcolor name="javadoc-tag" default="comment" foreColor="ff9393CC">
        <font style="bold"/>
    </fontcolor>
    
    <!-- enum -->
    <fontcolor name="mod-enum" default="identifier" foreColor="ff000000">
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="mod-enum-declaration" default="mod-enum" />
    
    <!-- interface -->
    <fontcolor name="mod-interface" default="identifier" foreColor="ff000000" />
    <fontcolor name="mod-interface-declaration" default="mod-interface">
        <font style="bold"/>
    </fontcolor>
    
    <!-- annotation -->
    <fontcolor name="mod-annotation-type" default="identifier" foreColor="ff000000">
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="mod-annotation-type-declaration" default="mod-annotation-type" />
    
    <!-- class -->
    <fontcolor name="mod-class" default="identifier" foreColor="ff000000"/>
    <fontcolor name="mod-class-declaration" default="mod-class">
        <font style="bold"/>
    </fontcolor>
    
    <!-- method / methodDeclaration -->
    <fontcolor name="mod-method" default="method" />
    <fontcolor name="mod-method-declaration" default="mod-method" >
        <font style="bold"/>
    </fontcolor>
     
    <!-- field / (FieldDeclaration) -->
    <fontcolor name="mod-field" default="field" />
    <fontcolor name="mod-field-declaration" default="mod-field" />

    <!-- (Static) [Netbeans does not seperate "staticField, staticFinalField, staticMethod"] -->
    <fontcolor name="mod-static">
        <font style="italic"/>
    </fontcolor>
    
    <!-- (String Escape) -->
    <fontcolor name="string-escape" default="string">
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="string-escape-invalid" default="string-escape" waveUnderlined="ffce7b00" />
    <!-- (Keywords) -->
    <fontcolor name="keyword-directive" default="keyword" />
    <fontcolor name="literal" default="keyword" />
    
    <!-- parameterVariable -->
    <fontcolor name="mod-parameter" default="identifier" />
    <fontcolor name="mod-parameter-declaration" default="mod-parameter" />
    
    <!-- (Constructor) -->
    <fontcolor name="mod-constructor" default="mod-method" />
    <fontcolor name="mod-constructor-declaration" default="mod-constructor" >
        <font style="bold"/>
    </fontcolor>
    
    <!-- localVariable / localVariableDeclaration -->
    <fontcolor name="mod-local-variable" default="identifier" foreColor="ff000000"/>
    <fontcolor name="mod-local-variable-declaration" default="mod-local-variable" foreColor="ff000000"/>

    
    <!-- (Abstract) [Eclipse keyword] -->
    <fontcolor name="mod-abstract"/>
    <!-- (Visibility) [Eclipse keyword] -->
    <fontcolor name="mod-private"/>
    <fontcolor name="mod-protected"/>
    <fontcolor name="mod-public"/>

    <!-- deprecatedMember [Eclipse overrides the foreColor] -->
    <fontcolor name="mod-deprecated" strikeThrough="ffFFFFFF"/>
    
    
    <!-- occurrenceIndication -->
    <fontcolor name="mark-occurrences" bgColor="ffeceba3" />
    
    <!-- (Misc) -->
    <fontcolor name="mod-unused" waveUnderlined="ff999999"/>
    <fontcolor name="mod-package-private"/>
    
    <!-- (Inherited from base) -->
    <fontcolor name="identifier" default="identifier" />
    <fontcolor name="comment" default="comment" />
    <fontcolor name="number" default="number" />
    <fontcolor name="separator" default="separator" />
    <fontcolor name="string" default="string" />
    <fontcolor name="character" default="char" />
    <fontcolor name="keyword" default="keyword" />
    <fontcolor name="whitespace" default="whitespace" />
    <fontcolor name="operator" default="operator" />
</fontscolors>
