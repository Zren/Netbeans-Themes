<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <!-- foreground / background -->
    <fontcolor name="default" bgColor="ff191919" foreColor="ffFFFFFF"/>
    <!-- string (& Character) -->
    <fontcolor name="char" foreColor="ff477488"/>
    <fontcolor name="string" foreColor="ff477488"/>
    <!-- operator -->
    <fontcolor name="operator" foreColor="ffFFFFFF"/>
    <!-- keyword -->
    <fontcolor name="keyword" foreColor="ffEC691E"/>
    <!-- bracket (Seperator) -->
    <fontcolor name="separator" foreColor="ffFFFFFF"/>
    <!-- number -->
    <fontcolor name="number" foreColor="ff477488"/>
    <!-- (Identifier) -->
    <fontcolor name="identifier"/>
    <!-- field -->
    <fontcolor name="field" default="identifier" foreColor="ff357A8F"/>
    <!-- method -->
    <fontcolor name="method" default="identifier" foreColor="ffF7C527"/>
    <!-- (Comment) -->
    <fontcolor name="comment" foreColor="ff8146A2" />
    
    <!-- (Effects) -->
    <fontcolor name="warning" waveUnderlined="yellow"/>
    <fontcolor name="error" waveUnderlined="red"/>
    <fontcolor name="url" underline="blue"/>
    
    <!-- (Whitespace) -->
    <fontcolor name="whitespace"/>
    
    
    <!--  -->
    <fontcolor name="markup-attribute" default="field" />
    <fontcolor name="markup-attribute-value" default="string" />
    <fontcolor name="markup-element" default="keyword" />
    <fontcolor name="entity-reference" default="default" foreColor="black">
        <font style="bold"/>
    </fontcolor>
</fontscolors>
