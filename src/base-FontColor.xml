<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <!-- foreground / background -->
    <fontcolor name="default" bgColor="ff000000" foreColor="ffF9F9F9"/>
    <!-- string (& Character) -->
    <fontcolor name="char" foreColor="ff76BA53"/>
    <fontcolor name="string" foreColor="ff76BA53"/>
    <!-- operator -->
    <fontcolor name="operator" foreColor="ffF9F9F9"/>
    <!-- keyword -->
    <fontcolor name="keyword" foreColor="ffEA9C77"/>
    <!-- bracket (Seperator) -->
    <fontcolor name="separator" foreColor="ffF9F9F9"/>
    <!-- number -->
    <fontcolor name="number" foreColor="ffF9F9F9"/>
    <!-- (Identifier) -->
    <fontcolor name="identifier"/>
    <!-- field -->
    <fontcolor name="field" default="identifier" foreColor="ff4B9CE9"/>
    <!-- method -->
    <fontcolor name="method" default="identifier" foreColor="ffF9F9F9"/>
    <!-- (Comment) -->
    <fontcolor name="comment" foreColor="ffA8A8A8" />
    
    <!-- (Effects) -->
    <fontcolor name="warning" waveUnderlined="yellow"/>
    <fontcolor name="error" waveUnderlined="red"/>
    <fontcolor name="url" underline="blue"/>
    
    <!-- (Whitespace) -->
    <fontcolor name="whitespace"/>
    
    
    <!--  -->
    <fontcolor name="markup-attribute" default="field" />
    <fontcolor name="markup-attribute-value" default="string" />
    <fontcolor name="markup-element" default="keyword" />
    <fontcolor name="entity-reference" default="default" foreColor="black">
        <font style="bold"/>
    </fontcolor>
</fontscolors>
